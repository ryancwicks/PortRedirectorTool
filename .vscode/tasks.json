{
	"version": "2.0.0",
	"tasks": [
		{
			"options": {
				"cwd": "port_redirector"
			},
			"type": "cargo",
			"command": "build",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "rust: cargo Debug build"
		},
		{
			"options": {
				"cwd": "port_redirector"
			},
			"type": "cargo",
			"command": "build",
			"args": "--release",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "rust: cargo Release build"
		}
	]
}